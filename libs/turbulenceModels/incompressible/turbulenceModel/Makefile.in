sources =turbulenceModelHolder.cpp



libname=libmanagedFlu-incompressibleTurbulenceModel

#--------------------------------------------------------------------------------------
__CPPFLAGS__ := $(__CPPFLAGS__) \
    -I/$@abs_top_builddir@/libs \
    -I/$@abs_top_builddir@/libs/managedFlu/lnInclude \
    -I/$@abs_top_builddir@/libs/OpenFOAM/lnInclude \
    -I/$@abs_top_builddir@/libs/finiteVolume/lnInclude \
    -I/$@abs_top_builddir@/libs/transportModels/incompressible/lnInclude \
    @OPENFOAM_FINITEVOLUME_CPPFLAGS@ \
    @OPENFOAM_INCOMPRESSIBLETRANSPORTMODELS_CPPFLAGS@ \
    @OPENFOAM_TRANSPORTMODELS_CPPFLAGS@ \
    @OPENFOAM_TURBULENCEMODELS_CPPFLAGS@ \
    @OPENFOAM_INCOMPRESSIBLETURBULENCEMODELS_CPPFLAGS@
	
__LDFLAGS__ := $(__LDFLAGS__) \
    @OPENFOAM_INCOMPRESSIBLETURBULENCEMODELS_LIBS@ \
    @OPENFOAM_FINITEVOLUME_LIBS@ \
    -L@abs_top_builddir@/lib -lmanagedFlu -lmanagedFlu-OpenFOAM -lmanagedFlu-finiteVolume -lmanagedFlu-incompressibleTransportModels

include @abs_top_builddir@/include.lib.makefile


